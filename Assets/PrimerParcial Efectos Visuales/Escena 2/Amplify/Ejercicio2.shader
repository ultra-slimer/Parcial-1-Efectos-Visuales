				float4 lerpResult78 = lerp( (tex2D( _SkyTexture, uv_SkyTexture )).rgba , float4( texCoord84, 0.0 , 0.0 ) , _FlowIntensity);
				float2 panner13 = ( _TimeParameters.x * _Speed + lerpResult78.rg);
