				float4 lerpResult18 = lerp( lerpResult72 , _Color2 , step( voroi62 , distanceDepth8 ));
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = lerpResult18.rgb;
